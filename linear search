def linear_search(arr, target):
    for i in range(len(arr)):
        if arr[i] == target:
            return i  # found at index i
    return -1  # not found

arr = [10, 20, 30, 40, 50]
target = 30
result = linear_search(arr, target)

print("Element found at index:" if result != -1 else "Element not found", result)
